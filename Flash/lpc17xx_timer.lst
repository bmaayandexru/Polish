N/*********************************************************************//**
N * @brief 		Get peripheral clock of each timer controller
N * @param[in]	timernum Timer number
N * @return 		Peripheral clock of timer
N **********************************************************************/
Nstatic uint32_t getPClock (uint32_t timernum)
N{
N	uint32_t clkdlycnt;
N	switch (timernum)
N	{
N	case 0:
N		clkdlycnt = CLKPWR_GetPCLK (CLKPWR_PCLKSEL_TIMER0);
X		clkdlycnt = CLKPWR_GetPCLK (((uint32_t)(2)));
N		break;
N
N	case 1:
N		clkdlycnt = CLKPWR_GetPCLK (CLKPWR_PCLKSEL_TIMER1);
X		clkdlycnt = CLKPWR_GetPCLK (((uint32_t)(4)));
N		break;
N
N	case 2:
N		clkdlycnt = CLKPWR_GetPCLK (CLKPWR_PCLKSEL_TIMER2);
X		clkdlycnt = CLKPWR_GetPCLK (((uint32_t)(44)));
N		break;
N
N	case 3:
N		clkdlycnt = CLKPWR_GetPCLK (CLKPWR_PCLKSEL_TIMER3);
X		clkdlycnt = CLKPWR_GetPCLK (((uint32_t)(46)));
N		break;
N	}
N	return clkdlycnt;
N}
N
N
N/*********************************************************************//**
N * @brief 		Convert a time to a timer count value
N * @param[in]	timernum Timer number
N * @param[in]	usec Time in microseconds
N * @return 		The number of required clock ticks to give the time delay
N **********************************************************************/
Nuint32_t converUSecToVal (uint32_t timernum, uint32_t usec)
N{
N	uint64_t clkdlycnt;
N
N	// Get Pclock of timer
N	clkdlycnt = (uint64_t) getPClock(timernum);
N
N	clkdlycnt = (clkdlycnt * usec) / 1000000;
N	return (uint32_t) clkdlycnt;
N}
N
N
N/*********************************************************************//**
N * @brief 		Convert a timer register pointer to a timer number
N * @param[in]	TIMx Pointer to LPC_TIM_TypeDef, should be:
N * 				- LPC_TIM0: TIMER0 peripheral
N * 				- LPC_TIM1: TIMER1 peripheral
N * 				- LPC_TIM2: TIMER2 peripheral
N * 				- LPC_TIM3: TIMER3 peripheral
N * @return 		The timer number (0 to 3) or -1 if register pointer is bad
N **********************************************************************/
Nuint32_t converPtrToTimeNum (LPC_TIM_TypeDef *TIMx)
N{
N	uint32_t tnum = 0xFFFFFFFF;
N
N	if (TIMx == LPC_TIM0)
X	if (TIMx == ((LPC_TIM_TypeDef *) ((0x40000000UL) + 0x04000) ))
N	{
N		tnum = 0;
N	}
N	else if (TIMx == LPC_TIM1)
X	else if (TIMx == ((LPC_TIM_TypeDef *) ((0x40000000UL) + 0x08000) ))
N	{
N		tnum = 1;
N	}
N	else if (TIMx == LPC_TIM2)
X	else if (TIMx == ((LPC_TIM_TypeDef *) ((0x40080000UL) + 0x10000) ))
N	{
N		tnum = 2;
N	}
N	else if (TIMx == LPC_TIM3)
X	else if (TIMx == ((LPC_TIM_TypeDef *) ((0x40080000UL) + 0x14000) ))
N	{
N		tnum = 3;
N	}
N
N	return tnum;
N}
