N/*********************************************************************//**
N * @brief 		Configuration for Match register
N * @param[in]	TIMx Pointer to timer device, should be:
N *   			- LPC_TIM0: TIMER0 peripheral
N * 				- LPC_TIM1: TIMER1 peripheral
N * 				- LPC_TIM2: TIMER2 peripheral
N * 				- LPC_TIM3: TIMER3 peripheral
N * @param[in]   TIM_MatchConfigStruct Pointer to TIM_MATCHCFG_Type
N * 					- MatchChannel : choose channel 0 or 1
N * 					- IntOnMatch	 : if SET, interrupt will be generated when MRxx match
N * 									the value in TC
N * 					- StopOnMatch	 : if SET, TC and PC will be stopped whenM Rxx match
N * 									the value in TC
N * 					- ResetOnMatch : if SET, Reset on MR0 when MRxx match
N * 									the value in TC
N * 					-ExtMatchOutputType: Select output for external match
N * 						 +	 0:	Do nothing for external output pin if match
N *						 +   1:	Force external output pin to low if match
N *						 + 	 2: Force external output pin to high if match
N *						 + 	 3: Toggle external output pin if match
N *					MatchValue: Set the value to be compared with TC value
N * @return 		None
N **********************************************************************/
Nvoid TIM_ConfigMatch(LPC_TIM_TypeDef *TIMx, TIM_MATCHCFG_Type *TIM_MatchConfigStruct)
N{
N
N	CHECK_PARAM(PARAM_TIMx(TIMx));
X	;
N	CHECK_PARAM(PARAM_TIM_EXTMATCH_OPT(TIM_MatchConfigStruct->ExtMatchOutputType));
X	;
N
N	switch(TIM_MatchConfigStruct->MatchChannel)
N	{
N	case 0:
N		TIMx->MR0 = TIM_MatchConfigStruct->MatchValue;
N		break;
N	case 1:
N		TIMx->MR1 = TIM_MatchConfigStruct->MatchValue;
N		break;
N	case 2:
N		TIMx->MR2 = TIM_MatchConfigStruct->MatchValue;
N		break;
N	case 3:
N		TIMx->MR3 = TIM_MatchConfigStruct->MatchValue;
N		break;
N	default:
N		//Error match value
N		//Error loop
N		while(1);
N	}
N	//interrupt on MRn
N	TIMx->MCR &=~TIM_MCR_CHANNEL_MASKBIT(TIM_MatchConfigStruct->MatchChannel);
X	TIMx->MCR &=~((uint32_t)(7<<(TIM_MatchConfigStruct->MatchChannel*3)));
N
N	if (TIM_MatchConfigStruct->IntOnMatch)
N		TIMx->MCR |= TIM_INT_ON_MATCH(TIM_MatchConfigStruct->MatchChannel);
X		TIMx->MCR |= ((1<<(TIM_MatchConfigStruct->MatchChannel * 3)));
N
N	//reset on MRn
N	if (TIM_MatchConfigStruct->ResetOnMatch)
N		TIMx->MCR |= TIM_RESET_ON_MATCH(TIM_MatchConfigStruct->MatchChannel);
X		TIMx->MCR |= ((1<<((TIM_MatchConfigStruct->MatchChannel * 3) + 1)));
N
N	//stop on MRn
N	if (TIM_MatchConfigStruct->StopOnMatch)
N		TIMx->MCR |= TIM_STOP_ON_MATCH(TIM_MatchConfigStruct->MatchChannel);
X		TIMx->MCR |= ((1<<((TIM_MatchConfigStruct->MatchChannel * 3) + 2)));
N
N	// match output type
N
N	TIMx->EMR 	&= ~TIM_EM_MASK(TIM_MatchConfigStruct->MatchChannel);
X	TIMx->EMR 	&= ~((0x03<<((TIM_MatchConfigStruct->MatchChannel << 1) + 4)));
N	TIMx->EMR   |= TIM_EM_SET(TIM_MatchConfigStruct->MatchChannel,TIM_MatchConfigStruct->ExtMatchOutputType);
X	TIMx->EMR   |= (((TIM_MatchConfigStruct->ExtMatchOutputType & 0x03)<<((TIM_MatchConfigStruct->MatchChannel << 1) + 4)));
N}
N/*********************************************************************//**
N * @brief 		Update Match value
N * @param[in]	TIMx Pointer to timer device, should be:
N *   			- LPC_TIM0: TIMER0 peripheral
N * 				- LPC_TIM1: TIMER1 peripheral
N * 				- LPC_TIM2: TIMER2 peripheral
N * 				- LPC_TIM3: TIMER3 peripheral
N * @param[in]	MatchChannel	Match channel, should be: 0..3
N * @param[in]	MatchValue		updated match value
N * @return 		None
N **********************************************************************/
Nvoid TIM_UpdateMatchValue(LPC_TIM_TypeDef *TIMx,uint8_t MatchChannel, uint32_t MatchValue)
N{
N	CHECK_PARAM(PARAM_TIMx(TIMx));
X	;
N	switch(MatchChannel)
N	{
N	case 0:
N		TIMx->MR0 = MatchValue;
N		break;
N	case 1:
N		TIMx->MR1 = MatchValue;
N		break;
N	case 2:
N		TIMx->MR2 = MatchValue;
N		break;
N	case 3:
N		TIMx->MR3 = MatchValue;
N		break;
N	default:
N		//Error Loop
N		while(1);
N	}
N
N}
N/*********************************************************************//**
N * @brief 		Configuration for Capture register
N * @param[in]	TIMx Pointer to timer device, should be:
N *   			- LPC_TIM0: TIMER0 peripheral
N * 				- LPC_TIM1: TIMER1 peripheral
N * 				- LPC_TIM2: TIMER2 peripheral
N * 				- LPC_TIM3: TIMER3 peripheral
N * 					- CaptureChannel: set the channel to capture data
N * 					- RisingEdge    : if SET, Capture at rising edge
N * 					- FallingEdge	: if SET, Capture at falling edge
N * 					- IntOnCaption  : if SET, Capture generate interrupt
N * @param[in]   TIM_CaptureConfigStruct	Pointer to TIM_CAPTURECFG_Type
N * @return 		None
N **********************************************************************/
Nvoid TIM_ConfigCapture(LPC_TIM_TypeDef *TIMx, TIM_CAPTURECFG_Type *TIM_CaptureConfigStruct)
N{
N
N	CHECK_PARAM(PARAM_TIMx(TIMx));
X	;
N	TIMx->CCR &= ~TIM_CCR_CHANNEL_MASKBIT(TIM_CaptureConfigStruct->CaptureChannel);
X	TIMx->CCR &= ~((uint32_t)(7<<(TIM_CaptureConfigStruct->CaptureChannel*3)));
N
N	if (TIM_CaptureConfigStruct->RisingEdge)
N		TIMx->CCR |= TIM_CAP_RISING(TIM_CaptureConfigStruct->CaptureChannel);
X		TIMx->CCR |= ((1<<(TIM_CaptureConfigStruct->CaptureChannel * 3)));
N
N	if (TIM_CaptureConfigStruct->FallingEdge)
N		TIMx->CCR |= TIM_CAP_FALLING(TIM_CaptureConfigStruct->CaptureChannel);
X		TIMx->CCR |= ((1<<((TIM_CaptureConfigStruct->CaptureChannel * 3) + 1)));
N
N	if (TIM_CaptureConfigStruct->IntOnCaption)
N		TIMx->CCR |= TIM_INT_ON_CAP(TIM_CaptureConfigStruct->CaptureChannel);
X		TIMx->CCR |= ((1<<((TIM_CaptureConfigStruct->CaptureChannel * 3) + 2)));
N}
