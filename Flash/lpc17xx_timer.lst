N/*********************************************************************//**
N * @brief	 	Start/Stop Timer/Counter device
N * @param[in]	TIMx Pointer to timer device, should be:
N *  			- LPC_TIM0: TIMER0 peripheral
N * 				- LPC_TIM1: TIMER1 peripheral
N * 				- LPC_TIM2: TIMER2 peripheral
N * 				- LPC_TIM3: TIMER3 peripheral
N * @param[in]	NewState
N * 				-	ENABLE  : set timer enable
N * 				-	DISABLE : disable timer
N * @return 		None
N **********************************************************************/
Nvoid TIM_Cmd(LPC_TIM_TypeDef *TIMx, FunctionalState NewState)
N{
N	CHECK_PARAM(PARAM_TIMx(TIMx));
X	;
N	if (NewState == ENABLE)
N	{
N		TIMx->TCR	|=  TIM_ENABLE;
X		TIMx->TCR	|=  ((uint32_t)(1<<0));
N	}
N	else
N	{
N		TIMx->TCR &= ~TIM_ENABLE;
X		TIMx->TCR &= ~((uint32_t)(1<<0));
N	}
N}
