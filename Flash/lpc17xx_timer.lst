N/*********************************************************************//**
N * @brief 		Initial Timer/Counter device
N * 				 	Set Clock frequency for Timer
N * 					Set initial configuration for Timer
N * @param[in]	TIMx  Timer selection, should be:
N * 				- LPC_TIM0: TIMER0 peripheral
N * 				- LPC_TIM1: TIMER1 peripheral
N * 				- LPC_TIM2: TIMER2 peripheral
N * 				- LPC_TIM3: TIMER3 peripheral
N * @param[in]	TimerCounterMode Timer counter mode, should be:
N * 				- TIM_TIMER_MODE: Timer mode
N * 				- TIM_COUNTER_RISING_MODE: Counter rising mode
N * 				- TIM_COUNTER_FALLING_MODE: Counter falling mode
N * 				- TIM_COUNTER_ANY_MODE:Counter on both edges
N * @param[in]	TIM_ConfigStruct pointer to TIM_TIMERCFG_Type
N * 				that contains the configuration information for the
N *                    specified Timer peripheral.
N * @return 		None
N **********************************************************************/
Nvoid TIM_Init(LPC_TIM_TypeDef *TIMx, TIM_MODE_OPT TimerCounterMode, void *TIM_ConfigStruct)
N{
N	TIM_TIMERCFG_Type *pTimeCfg;
N	TIM_COUNTERCFG_Type *pCounterCfg;
N
N	CHECK_PARAM(PARAM_TIMx(TIMx));
X	;
N	CHECK_PARAM(PARAM_TIM_MODE_OPT(TimerCounterMode));
X	;
N
N	//set power
N
N	if (TIMx== LPC_TIM0)
X	if (TIMx== ((LPC_TIM_TypeDef *) ((0x40000000UL) + 0x04000) ))
N	{
N		CLKPWR_ConfigPPWR (CLKPWR_PCONP_PCTIM0, ENABLE);
X		CLKPWR_ConfigPPWR (((uint32_t)(1<<1)), ENABLE);
N		//PCLK_Timer0 = CCLK/4
N		CLKPWR_SetPCLKDiv (CLKPWR_PCLKSEL_TIMER0, CLKPWR_PCLKSEL_CCLK_DIV_4);
X		CLKPWR_SetPCLKDiv (((uint32_t)(2)), ((uint32_t)(0)));
N	}
N	else if (TIMx== LPC_TIM1)
X	else if (TIMx== ((LPC_TIM_TypeDef *) ((0x40000000UL) + 0x08000) ))
N	{
N		CLKPWR_ConfigPPWR (CLKPWR_PCONP_PCTIM1, ENABLE);
X		CLKPWR_ConfigPPWR (((uint32_t)(1<<2)), ENABLE);
N		//PCLK_Timer1 = CCLK/4			 4					  0
N		CLKPWR_SetPCLKDiv (CLKPWR_PCLKSEL_TIMER1, CLKPWR_PCLKSEL_CCLK_DIV_1);
X		CLKPWR_SetPCLKDiv (((uint32_t)(4)), ((uint32_t)(1)));
N
N	}
N
N	else if (TIMx== LPC_TIM2)
X	else if (TIMx== ((LPC_TIM_TypeDef *) ((0x40080000UL) + 0x10000) ))
N	{
N		CLKPWR_ConfigPPWR (CLKPWR_PCONP_PCTIM2, ENABLE);
X		CLKPWR_ConfigPPWR (((uint32_t)(1<<22)), ENABLE);
N		//PCLK_Timer2= CCLK/4
N		CLKPWR_SetPCLKDiv (CLKPWR_PCLKSEL_TIMER2, CLKPWR_PCLKSEL_CCLK_DIV_4);
X		CLKPWR_SetPCLKDiv (((uint32_t)(44)), ((uint32_t)(0)));
N	}
N	else if (TIMx== LPC_TIM3)
X	else if (TIMx== ((LPC_TIM_TypeDef *) ((0x40080000UL) + 0x14000) ))
N	{
N		CLKPWR_ConfigPPWR (CLKPWR_PCONP_PCTIM3, ENABLE);
X		CLKPWR_ConfigPPWR (((uint32_t)(1<<23)), ENABLE);
N		//PCLK_Timer3= CCLK/4
N		CLKPWR_SetPCLKDiv (CLKPWR_PCLKSEL_TIMER3, CLKPWR_PCLKSEL_CCLK_DIV_4);
X		CLKPWR_SetPCLKDiv (((uint32_t)(46)), ((uint32_t)(0)));
N
N	}
N
N	TIMx->CCR &= ~TIM_CTCR_MODE_MASK;
X	TIMx->CCR &= ~0x3;
N	TIMx->CCR |= TIM_TIMER_MODE;
N
N	TIMx->TC =0;
N	TIMx->PC =0;
N	TIMx->PR =0;
N	TIMx->TCR |= (1<<1); //Reset Counter
N	TIMx->TCR &= ~(1<<1); //release reset
N	if (TimerCounterMode == TIM_TIMER_MODE )
N	{
N		pTimeCfg = (TIM_TIMERCFG_Type *)TIM_ConfigStruct;
N		if (pTimeCfg->PrescaleOption  == TIM_PRESCALE_TICKVAL)
N		{
N			TIMx->PR   = pTimeCfg->PrescaleValue -1  ;
N		}
N		else
N		{
N			TIMx->PR   = converUSecToVal (converPtrToTimeNum(TIMx),pTimeCfg->PrescaleValue)-1;
N		}
N	}
N	else
N	{
N
N		pCounterCfg = (TIM_COUNTERCFG_Type *)TIM_ConfigStruct;
N		TIMx->CCR  &= ~TIM_CTCR_INPUT_MASK;
X		TIMx->CCR  &= ~0xC;
N		if (pCounterCfg->CountInputSelect == TIM_COUNTER_INCAP1)
N			TIMx->CCR |= _BIT(2);
X			TIMx->CCR |= (1<<2);
N	}
N
N	// Clear interrupt pending
N	TIMx->IR = 0xFFFFFFFF;
N
N}
