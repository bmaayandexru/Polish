N * 				- TIM_TIMER_MODE: Timer mode
N * 				- TIM_COUNTER_RISING_MODE: Counter rising mode
N * 				- TIM_COUNTER_FALLING_MODE: Counter falling mode
N * 				- TIM_COUNTER_ANY_MODE:Counter on both edges
N * @param[in] 	TIM_ConfigStruct pointer to TIM_TIMERCFG_Type or
N * 				TIM_COUNTERCFG_Type
N * @return 		None
N **********************************************************************/
Nvoid TIM_ConfigStructInit(TIM_MODE_OPT TimerCounterMode, void *TIM_ConfigStruct)
N{
N	if (TimerCounterMode == TIM_TIMER_MODE )
N	{
N		TIM_TIMERCFG_Type * pTimeCfg = (TIM_TIMERCFG_Type *)TIM_ConfigStruct;
N		pTimeCfg->PrescaleOption = TIM_PRESCALE_USVAL;
N		pTimeCfg->PrescaleValue = 1;
N	}
N	else
N	{
N		TIM_COUNTERCFG_Type * pCounterCfg = (TIM_COUNTERCFG_Type *)TIM_ConfigStruct;
N		pCounterCfg->CountInputSelect = TIM_COUNTER_INCAP0;
N	}
N}
