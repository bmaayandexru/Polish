N
N	else if (TIMx== LPC_TIM1)
X	else if (TIMx== ((LPC_TIM_TypeDef *) ((0x40000000UL) + 0x08000) ))
N		CLKPWR_ConfigPPWR (CLKPWR_PCONP_PCTIM1, DISABLE);
X		CLKPWR_ConfigPPWR (((uint32_t)(1<<2)), DISABLE);
N
N	else if (TIMx== LPC_TIM2)
X	else if (TIMx== ((LPC_TIM_TypeDef *) ((0x40080000UL) + 0x10000) ))
N		CLKPWR_ConfigPPWR (CLKPWR_PCONP_PCTIM2, DISABLE);
X		CLKPWR_ConfigPPWR (((uint32_t)(1<<22)), DISABLE);
N
N	else if (TIMx== LPC_TIM3)
