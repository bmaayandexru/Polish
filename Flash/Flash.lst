N//*****************************************
N//          SSP0 Send Byte
N//*****************************************
Nu8 SSP0_SendByte(u8 b) {
N	u8 result;
N	LPC_SSP0->DR = b;
X	((LPC_SSP_TypeDef *) ((0x40080000UL) + 0x08000) )->DR = b;
N	while ( SSP0_GetStatusFlag(BSY) );							// Wait for transfer to finish
X	while ( SSP0_GetStatusFlag(0x10) );							
N	result = LPC_SSP0->DR;
X	result = ((LPC_SSP_TypeDef *) ((0x40080000UL) + 0x08000) )->DR;
N	return (result);											// Return received value
N}
