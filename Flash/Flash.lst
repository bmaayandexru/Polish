N//---------------------------------------------------------
N//                     FLASH _ Init
N//---------------------------------------------------------
N// Инициализация флэш-памяти
N// Здесь проверяется яналичие и емкость каждго банка,
N//   вычисляется размер каждого банка и общий размер флэш-памяти (количество байт)
Nvoid FLASH_Init(void) {
N	u32 tmp;
N
N	//SPI_Init();
N	SSP0_Init();
N
N	FLASH_BankCapacity[0] = FLASH_GetBankCapacity(0);
N	FLASH_BankCapacity[1] = FLASH_GetBankCapacity(1);
N	FLASH_BankCapacity[2] = FLASH_GetBankCapacity(2);
N	FLASH_BankCapacity[3] = FLASH_GetBankCapacity(3);
N
N	FLASH_BankSize = FLASH_BankCapacity[0] * PAGESIZE;			// Сохоранить размер банка (количество байт)
X	FLASH_BankSize = FLASH_BankCapacity[0] * 256;			
N
N	tmp  = FLASH_BankCapacity[0];
N	tmp += FLASH_BankCapacity[1];
N	tmp += FLASH_BankCapacity[2];
N	tmp += FLASH_BankCapacity[3];
N
N	FLASH_TotalSize = tmp * PAGESIZE;							// Сохранить общий размер флэш-памяти (количество байт)
X	FLASH_TotalSize = tmp * 256;							
N}
N
