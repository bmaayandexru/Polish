N//*****************************************
N//        SSP0 Get Status Flag
N//*****************************************
Nu8 SSP0_GetStatusFlag(u8 flag) {
N	return (LPC_SSP0->SR & flag);
X	return (((LPC_SSP_TypeDef *) ((0x40080000UL) + 0x08000) )->SR & flag);
N}
