N//                 FLASH _ Read Status Register
N//---------------------------------------------------------
N// Функция читает и возвращает регистр статуса заданного банка флэш-памяти
Nu8 FLASH_ReadStatusRegister(u8 bank_num) {
N	u8 result;
N
N	FLASH_BankSelect(bank_num);									// Выбрать банк
N	FLASH_SendByte(RDSR);										// Отправить команду "Read Status Register"
X	FLASH_SendByte(0x05);										
N	result = FLASH_ReceiveByte();								// Принять ответный байт
N	FLASH_BankDeselect(bank_num);								// Снять выбор банка
N
N	return result;
N}
