N// Select Bank - Chip Select to low
Nvoid FLASH_BankSelect(u8 bank_num) {
N	switch (bank_num) {
N		case 0: // Select Bank 0 - CS0 low
N			LPC_GPIO0->PORT_CLEAR |= FLASH_CS0;
X			((LPC_GPIO_TypeDef *) ((0x2009C000UL) + 0x00000) )->FIOCLR |= (1UL << 24);
N			break;
N		case 1: // Select Bank 1 - CS1 low
N			LPC_GPIO0->PORT_CLEAR |= FLASH_CS1;
X			((LPC_GPIO_TypeDef *) ((0x2009C000UL) + 0x00000) )->FIOCLR |= (1UL << 25);
N			break;
N		case 2: // Select Bank 2 - CS2 low
N			LPC_GPIO0->PORT_CLEAR |= FLASH_CS2;
X			((LPC_GPIO_TypeDef *) ((0x2009C000UL) + 0x00000) )->FIOCLR |= (1UL << 26);
N			break;
N		case 3: // Select Bank 3 - CS3 low
N			LPC_GPIO0->PORT_CLEAR |= FLASH_CS3;
X			((LPC_GPIO_TypeDef *) ((0x2009C000UL) + 0x00000) )->FIOCLR |= (1UL << 27);
N			break;
N		case 4: // Select Bank 4 - CS4 low
N			LPC_GPIO0->PORT_CLEAR |= FLASH_CS4;
X			((LPC_GPIO_TypeDef *) ((0x2009C000UL) + 0x00000) )->FIOCLR |= (1UL << 28);
N			break;
N	}
N}
