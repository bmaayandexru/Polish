N//---------------------------------------------------------
N//                 FLASH _ Sector Erase
N//---------------------------------------------------------
Nu8 FLASH_SectorErase(u32 addr) {
N	u8 bank_num;
N	u32 addr_in_bank;
N
N	bank_num = FLASH_BankNumber(addr);
N	addr_in_bank = FLASH_AddressInBank(addr);
N
N	FLASH_BankSelect(bank_num);									// Select Bank
N	FLASH_SendByte(WREN);										// Send "Write Enable" instruction
X	FLASH_SendByte(0x06);										
N	FLASH_BankDeselect(bank_num);								// Deselect Bank
N
N	FLASH_BankSelect(bank_num);									// Select Bank
N	FLASH_SendByte(SE);											// Send "Sector Erase" instruction
X	FLASH_SendByte(0xD8);											
N	FLASH_SendByte((u8)(addr_in_bank >> 16));					// Send Write Address high byte
N	FLASH_SendByte((u8)(addr_in_bank >> 8));					// Send Write Address medium byte
N	FLASH_SendByte((u8)addr_in_bank);							// Send Write Address low byte
N	FLASH_BankDeselect(bank_num);								// Deselect Bank
N
N	FLASH_BankSelect(bank_num);									// Select Bank
N	FLASH_SendByte(RDSR);										// Send "Read Status Register" instruction
X	FLASH_SendByte(0x05);										
N	while ( FLASH_ReceiveByte() & FLASH_WIP );					// Write in progress
X	while ( FLASH_ReceiveByte() & (1 << 0) );					
N	FLASH_BankDeselect(bank_num);								// Deselect Bank
NMsg("complete SectorErase\r\n");//<!>
NSMsg();//<!>
NW=W1;while(W--);//<!>
N
N	return 0;
N}
N
N
N//---------------------------------------------------------
N//                  FLASH _ Bulk Erase
N//---------------------------------------------------------
Nu32 FLASH_BulkErase(void) {
N	u8 i;
N	u32 ret[4] = {55,55,55,55};
N
N	for (i=0; i<4; i++) {
N		Msg("Bulk erasing bank %d...\r\n",i); SMsg();
N		if ( FLASH_BankCapacity[i] ) {
N			FLASH_BankSelect(i);								// Select Bank
N			FLASH_SendByte(WREN);								// Send "Write Enable" instruction
X			FLASH_SendByte(0x06);								
N			FLASH_BankDeselect(i);								// Deselect Bank
N
N			FLASH_BankSelect(i);								// Select Bank
N			FLASH_SendByte(BE);									// Send "Bulk Erase" instruction
X			FLASH_SendByte(0xC7);									
N			FLASH_BankDeselect(i);								// Deselect Bank
N		}
N	}
N
N	for (i=0; i<4; i++) {
N		Msg("Wait erasing bank %d...\r\n",i); SMsg();
N		if ( FLASH_BankCapacity[i] )
N			ret[i] = FLASH_WaitBulkErasing(i);					// Wait for Flash erasing
N		Msg("Complete bank %d.\r\n",i); SMsg();
N	}
N
N	return ret[0];
N}
