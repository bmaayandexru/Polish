N//---------------------------------------------------------
N//                 FLASH _ Wait Writing
N//---------------------------------------------------------
N// Ожидание завершения записи заданного банка флэш-памяти
N// Функция возвращает результат ожидания:
N//   1 - OK (ожидание записи завершилось успешно)
N//   0 - Ошибка (ожидание записи прервано по тайм-ауту)
Nu8 FLASH_WaitWriting(u8 bank_num) {
N//	u32 i = 0;
N	u8 result = 1;
N
N	FLASH_BankSelect(bank_num);									// Выбрать банк
N	FLASH_SendByte(RDSR);												// Отправить команду "Read Status Register"
X	FLASH_SendByte(0x05);												
N	while ( FLASH_ReceiveByte() & FLASH_WIP ) {	// Выполняется запись - ожидаем завершения
X	while ( FLASH_ReceiveByte() & (1 << 0) ) {	
N//		if ( i++ > 250 ) {											// Считаем циклы ожидания - для избежания зацикливания
N//			result = 0;
N//			break;
N//		}
N	}
N	FLASH_BankDeselect(bank_num);								// Снять выбор банка
N
N	return result;
N}
