N/*
N Full memory is 4x M25P64:
N 0000000bbpppppppppppppppxxxxxxxx - "bb" is bank number, "ppppppppppppppp" is page number
N 0000000bbsssssssxxxxxxxxxxxxxxxx - "bb" is bank number, "sssssss" is sector number
N 0000000bbaaaaaaaaaaaaaaaaaaaaaaa - "bb" is bank number, "aaaaaaaaaaaaaaaaaaaaaaa" is address in bank
N
N Full memory is 4x M25P128:
N 000000bbppppppppppppppppxxxxxxxx - "bb" is bank number, "pppppppppppppppp" is page number
N 000000bbssssssxxxxxxxxxxxxxxxxxx - "bb" is bank number, "ssssss" is sector number
N 000000bbaaaaaaaaaaaaaaaaaaaaaaaa - "bb" is bank number, "aaaaaaaaaaaaaaaaaaaaaaaa" is address in bank
N*/
N
N
N//---------------------------------------------------------
N//                 FLASH _ Get Bank Number
N//---------------------------------------------------------
Nu8 FLASH_BankNumber(u32 addr) {
N	if (FLASH_BankCapacity[0] == 32768) {						// Flash type is M25P64
N		addr >>= 23;
N	}
N	else {														// Flash type is M25P128
N		addr >>= 24;
N	}
N
N	return (u8)(addr);
N}
N
N
N//---------------------------------------------------------
N//                  FLASH _ Get Bank Address
N//---------------------------------------------------------
Nu32 FLASH_AddressInBank(u32 addr) {
N	if (FLASH_BankCapacity[0] == 32768) {						// If Flash type is M25P64
N		addr &= 0x007FFFFF;										// then Flash address is 23 low bits of full address
N	}
N	else {														// If Flash type is M25P64
N		addr &= 0x00FFFFFF;										// and Flash number is 24 low bits of full address
N	}
N
N	return addr;
N}
N
N
N//---------------------------------------------------------
N//                 FLASH _ Get Page Address
N//---------------------------------------------------------
Nu32 FLASH_PageAddress(u32 addr) {
N	return addr &= 0x000000FF;									// Page address is 8 low bits of address
N}																// (for page size of 256 bytes)
N
N
N//---------------------------------------------------------
N//              FLASH _ Записать 1 байт
N//---------------------------------------------------------
N// Функция возвращает результат записи:
N//   0 - OK (запись завершилась успешно)
N//   1 - Ошибка (ожидание записи прервано по тайм-ауту)
Nu8 FLASH_WriteByte(u32 addr, u8 b) {
N	u8 bank_num;
N	u32 addr_in_bank;
N
N	bank_num = FLASH_BankNumber(addr);
N	addr_in_bank = FLASH_AddressInBank(addr);
N
N	FLASH_BankSelect(bank_num);									// Select Bank
N	FLASH_SendByte(WREN);										// Send "Write Enable" instruction
X	FLASH_SendByte(0x06);										
N	FLASH_BankDeselect(bank_num);								// Deselect Bank
N
N	FLASH_BankSelect(bank_num);									// Select Bank
N	FLASH_SendByte(PP);											// Send "Page Program" instruction
X	FLASH_SendByte(0x02);											
N	FLASH_SendByte((u8)(addr_in_bank >> 16));					// Send Write Address high byte
N	FLASH_SendByte((u8)(addr_in_bank >> 8));					// Send Write Address medium byte
N	FLASH_SendByte((u8)addr_in_bank);							// Send Write Address low byte
N	FLASH_SendByte(b);											// Send data byte
N	FLASH_BankDeselect(bank_num);								// Deselect Bank
N
N	return FLASH_WaitWriting(bank_num);							// Wait for Flash writing
N}
N
N
N//---------------------------------------------------------
N//              FLASH _ Прочитать 1 байт
N//---------------------------------------------------------
Nu8 FLASH_ReadByte(u32 addr) {
N	u8 bank_num;
N	u32 addr_in_bank;
N	u8 b;
N
N	bank_num = FLASH_BankNumber(addr);
N	addr_in_bank = FLASH_AddressInBank(addr);
N	FLASH_BankSelect(bank_num);									// Select Bank
N	FLASH_SendByte(READ);										// Send "Read Data Bytes" instruction
X	FLASH_SendByte(0x03);										
N	FLASH_SendByte((u8)(addr_in_bank >> 16));					// Send Read Address high byte
N	FLASH_SendByte((u8)(addr_in_bank >> 8));					// Send Read Address medium byte
N	FLASH_SendByte((u8)addr_in_bank);							// Send Read Address low byte
N	b = FLASH_ReceiveByte();									// Read data byte
N	FLASH_BankDeselect(bank_num);								// Deselect Bank
N
N	return b;
N}
N
N
N//---------------------------------------------------------
N//                  FLASH _ Page Write
N//---------------------------------------------------------
N// Функция возвращает результат ожидания записи страницы:
N//   1 - OK (ожидание записи завершилось успешно)
N//   0 - Ошибка (ожидание записи прервано по тайм-ауту)
Nu8 FLASH_PageWrite(u32 addr, const u8 *buffer, u16 size) {
N	u8 bank_num;
N	u32 addr_in_bank;
N
N	bank_num = FLASH_BankNumber(addr);
N	addr_in_bank = FLASH_AddressInBank(addr);
N
N	FLASH_BankSelect(bank_num);									// Select Bank
N	FLASH_SendByte(WREN);										// Send "Write Enable" instruction
X	FLASH_SendByte(0x06);										
N	FLASH_BankDeselect(bank_num);								// Deselect Bank
N
N	FLASH_BankSelect(bank_num);									// Select Bank
N	FLASH_SendByte(PP);											// Send "Page Program" instruction
X	FLASH_SendByte(0x02);											
N	FLASH_SendByte((u8)(addr_in_bank >> 16));					// Send Write Address high byte
N	FLASH_SendByte((u8)(addr_in_bank >> 8));					// Send Write Address medium byte
N	FLASH_SendByte((u8)addr_in_bank);							// Send Write Address low byte
N	while(size--) {												// While there is data to be written on FLASH
N		FLASH_SendByte(*buffer++);								// Send next byte
N	}
N	FLASH_BankDeselect(bank_num);								// Deselect Bank
N
N	return FLASH_WaitWriting(bank_num);							// Wait for Flash writing
N}
N
N
N//---------------------------------------------------------
N//                 FLASH _ Write
N//---------------------------------------------------------
Nu8 FLASH_Write(u32 addr, const u8 *buffer, u32 size) {
N	u8 first_size;												// Size of first page
N	u8 last_size;												// Size of last page
N	u32 full_page_count;										// Count of full pages
N	u8 result = 1;
N
N 	if (!(u8)addr) {											// Size of first page
N		first_size = 0;
N	}
N	else {
N		first_size = 256 - (u8)addr;
N	}
N	if (first_size > size) {
N		first_size = size;
N	}
N	full_page_count = (size - first_size) / 256;				// Count of full pages
N	last_size = (size - first_size) % 256;						// Size of last page
N
N	// Write first page
N	if (first_size) {
N		result = FLASH_PageWrite(addr, buffer, first_size);
N		buffer += first_size;
N		addr += first_size;
N	}
N
N	// Write full pages
N	while (result && full_page_count--) {
N		result = FLASH_PageWrite(addr, buffer, 256);
N		buffer += 256;
N		addr += 256;
N	}
N
N	// Write last page
N	if (result && last_size) {
N		result = FLASH_PageWrite(addr, buffer, last_size);
N	}
N
N	return result;
N}
N
