N//---------------------------------------------------------
N//                 FLASH _ Get Сapacity
N//---------------------------------------------------------
N// Получить емкость заданного банка флэш-памяти (количество страниц)
Nu32 FLASH_GetBankCapacity(u8 bank_num) {						// (bank_num = номер банка флэш-памяти, 0..3)
N	u8 tmp;
N	u32 capacity;
N
N	FLASH_BankSelect(bank_num);									// Выбрать банк
N	FLASH_SendByte(RDID);										// Отправить команду "Read Identification"
X	FLASH_SendByte(0x9F);										
N	tmp = FLASH_ReceiveByte();									// Принять первый байт - игнорировать его
N	tmp = FLASH_ReceiveByte();									// Принять второй байт - игнорировать его
N	tmp = FLASH_ReceiveByte();									// Принять третитий байт = это ID выбранного банка
N	FLASH_BankDeselect(bank_num);								// Снять выбор банка
N	if (tmp == 0x17) {											// Если ID = 17h (тип флэш-памяти M25P64)
N		capacity = 32768;										//   значит емкость банка 32768 страниц
N	}
N	else if (tmp == 0x18) {										// Если ID = 18h (тип флэш-памяти M25P128)
N		capacity = 65536;										//   значит емкость банка 65536 страниц
N	}
N	else {
N		capacity = 0;											// Иначе - считаем, что выбранный банк отсутствует (емкость 0 страниц)
N	}
N	return capacity;
N}
