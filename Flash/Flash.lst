N//---------------------------------------------------------
N//                  FLASH _ Bank Read
N//---------------------------------------------------------
Nvoid FLASH_BankRead(u8 *buffer, u32 addr, u32 size) {
N	u8 bank_num;
N	u32 addr_in_bank;
N
N	bank_num = FLASH_BankNumber(addr);
N	addr_in_bank = FLASH_AddressInBank(addr);
N
N	FLASH_BankSelect(bank_num);									// Select Bank
N	FLASH_SendByte(READ);										// Send "Read Data Bytes" instruction
X	FLASH_SendByte(0x03);										
N	FLASH_SendByte((u8)(addr_in_bank >> 16));					// Send Read Address high byte
N	FLASH_SendByte((u8)(addr_in_bank >> 8));					// Send Read Address medium byte
N	FLASH_SendByte((u8)addr_in_bank);							// Send Read Address low byte
N	while(size--) {												// While there is data to be read from FLASH
N		*buffer++ = FLASH_ReceiveByte();						// Read next byte
N	}
N	FLASH_BankDeselect(bank_num);								// Deselect Bank
N}
N
N
N//---------------------------------------------------------
N//                 FLASH _ Read
N//---------------------------------------------------------
Nvoid FLASH_Read(u8 *buffer, u32 addr, u32 size) {
N	u32 first_size;												// Size of first bank
N	u32 last_size;												// Size of last bank
N	u8 full_bank_count;											// Count of full banks
N
N	first_size = FLASH_BankSize - FLASH_AddressInBank(addr);	// Size of first bank
N	if (first_size == FLASH_BankSize) {
N		first_size = 0;
N	}
N	if (first_size > size) {
N		first_size = size;
N	}
N	full_bank_count = (size - first_size) / FLASH_BankSize;		// Count of full banks
N	last_size = (size - first_size) % FLASH_BankSize;			// Size of last bank
N
N	if (first_size) {
N		FLASH_BankRead(buffer, addr, first_size);				// Write first bank
N		buffer += first_size;
N		addr += first_size;
N	}
N
N	while (full_bank_count--) {									// Write full banks
N		FLASH_BankRead(buffer, addr, FLASH_BankSize);
N		buffer += FLASH_BankSize;
N		addr += FLASH_BankSize;
N	}
N
N	if (last_size) {
N		FLASH_BankRead(buffer, addr, last_size);				// Write last bank
N	}
N}
