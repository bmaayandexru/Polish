Nvoid SSP0_Init(void) {
N	LPC_SC->PCONP       |= (1 << 21);				/* Enable power to SSPI0 block */
X	((LPC_SC_TypeDef *) ((0x40080000UL) + 0x7C000) )->PCONP       |= (1 << 21);				 
N
N	/* SSEL is GPIO, output set to high. */
N	LPC_GPIO0->FIODIR   |=  (1<<16);				/* P0.16 is output             */
X	((LPC_GPIO_TypeDef *) ((0x2009C000UL) + 0x00000) )->FIODIR   |=  (1<<16);				 
N	LPC_GPIO0->FIOPIN   |=  (1<<16);				/* set P0.16 high (SSEL inact.)*/
X	((LPC_GPIO_TypeDef *) ((0x2009C000UL) + 0x00000) )->FIOPIN   |=  (1<<16);				 
N	LPC_PINCON->PINSEL1 &= ~(3<<0);					/* P0.16 SSEL (used as GPIO)   */
X	((LPC_PINCON_TypeDef *) ((0x40000000UL) + 0x2C000) )->PINSEL1 &= ~(3<<0);					 
N
N	/* SCK, MISO, MOSI are SSP pins. */
N	LPC_PINCON->PINSEL0 &= ~(3UL<<30);				/* P0.15 cleared               */
X	((LPC_PINCON_TypeDef *) ((0x40000000UL) + 0x2C000) )->PINSEL0 &= ~(3UL<<30);				 
N	LPC_PINCON->PINSEL0 |=  (2UL<<30);				/* P0.15 SCK0                  */
X	((LPC_PINCON_TypeDef *) ((0x40000000UL) + 0x2C000) )->PINSEL0 |=  (2UL<<30);				 
N	LPC_PINCON->PINSEL1 &= ~((3<<2) | (3<<4));		/* P0.17, P0.18 cleared        */
X	((LPC_PINCON_TypeDef *) ((0x40000000UL) + 0x2C000) )->PINSEL1 &= ~((3<<2) | (3<<4));		 
N	LPC_PINCON->PINSEL1 |=  ((2<<2) | (2<<4));		/* P0.17 MISO0, P0.18 MOSI0    */
X	((LPC_PINCON_TypeDef *) ((0x40000000UL) + 0x2C000) )->PINSEL1 |=  ((2<<2) | (2<<4));		 
N
N	LPC_SC->PCLKSEL1    &= ~(3<<10);				/* PCLKSP0 = CCLK/4 ( 25MHz)   */
X	((LPC_SC_TypeDef *) ((0x40080000UL) + 0x7C000) )->PCLKSEL1    &= ~(3<<10);				 
N	LPC_SC->PCLKSEL1    |=  (1<<10);				/* PCLKSP0 = CCLK   (100MHz)   */
X	((LPC_SC_TypeDef *) ((0x40080000UL) + 0x7C000) )->PCLKSEL1    |=  (1<<10);				 
N
N	LPC_SSP0->CPSR       = 10;						/* 100MHz / 10 = 10MHz         */
X	((LPC_SSP_TypeDef *) ((0x40080000UL) + 0x08000) )->CPSR       = 10;						 
N													/* maximum of 18MHz is possible*/    
N	LPC_SSP0->CR0        = 0x0007;					/* 8Bit, SPI format, CPOL=0, CPHA=0 */
X	((LPC_SSP_TypeDef *) ((0x40080000UL) + 0x08000) )->CR0        = 0x0007;					 
N	LPC_SSP0->CR1        = 0x0002;					/* SSP0 enable, master         */
X	((LPC_SSP_TypeDef *) ((0x40080000UL) + 0x08000) )->CR1        = 0x0002;					 
N
N	/* Pin configure to SPI Chip Select lines */
N	LPC_PINCON->PORT0_16_to_31_FUNCTION  |= PIN24_as_GPIO |		// CS0
X	((LPC_PINCON_TypeDef *) ((0x40000000UL) + 0x2C000) )->PINSEL1  |= (0UL << 16) |		
N                                            PIN25_as_GPIO |		// CS1
X                                            (0UL << 18) |		
N                                            PIN26_as_GPIO |		// CS2
X                                            (0UL << 20) |		
N                                            PIN27_as_GPIO |		// CS3
X                                            (0UL << 22) |		
N                                            PIN28_as_GPIO;		// CS4
X                                            (0UL << 24);		
N
N	/* SC Internal pull configure */
N	LPC_PINCON->PORT0_16_to_31_PULL_MODE |= PIN24_NOPULL |
X	((LPC_PINCON_TypeDef *) ((0x40000000UL) + 0x2C000) )->PINMODE1 |= (2UL << 16) |
N                                            PIN25_NOPULL |
X                                            (2UL << 18) |
N                                            PIN26_NOPULL |
X                                            (2UL << 20) |
N                                            PIN27_NOPULL |
X                                            (2UL << 22) |
N                                            PIN28_NOPULL;
X                                            (2UL << 24);
N
N	/* CS direction */
N	LPC_GPIO0->PORT_DIRECTION            |= PIN24_OUTPUT |
X	((LPC_GPIO_TypeDef *) ((0x2009C000UL) + 0x00000) )->FIODIR            |= (1UL << 24) |
N                                            PIN25_OUTPUT |
X                                            (1UL << 25) |
N                                            PIN26_OUTPUT |
X                                            (1UL << 26) |
N                                            PIN27_OUTPUT |
X                                            (1UL << 27) |
N                                            PIN28_OUTPUT;
X                                            (1UL << 28);
N
N	/* Set CS (inactive state) */
N	LPC_GPIO0->PORT_SET                  |= FLASH_CS0 |
X	((LPC_GPIO_TypeDef *) ((0x2009C000UL) + 0x00000) )->FIOSET                  |= (1UL << 24) |
N                                            FLASH_CS1 |
X                                            (1UL << 25) |
N                                            FLASH_CS2 |
X                                            (1UL << 26) |
N                                            FLASH_CS3 |
X                                            (1UL << 27) |
N                                            FLASH_CS4;
X                                            (1UL << 28);
N}
