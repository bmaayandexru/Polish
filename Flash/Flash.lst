N//*****************************************
N//              SPI Init
N//*****************************************
Nvoid SPI_Init(void) {
N	/* Enable AHB clock to SPI */
N	LPC_SC->POWER_CONTROL = GPIO_POWERED |
X	((LPC_SC_TypeDef *) ((0x40080000UL) + 0x7C000) )->PCONP = (1UL << 15) |
N                            SPI_POWERED;
X                            (1UL << 8);
N
N	/* Divider SPI clock. Using default divided by 4 */
N	LPC_SC->PCLKSEL0 &= ~(0x3 << 16);
X	((LPC_SC_TypeDef *) ((0x40080000UL) + 0x7C000) )->PCLKSEL0 &= ~(0x3 << 16);
N
N	/* SPI Clock Counter Register. 0 for Master mode */
N	LPC_SPI->SPCCR = 0;
X	((LPC_SPI_TypeDef *) ((0x40000000UL) + 0x20000) )->SPCCR = 0;
N
N	/* Pin configure to SPI functions */
N	// SSEL functin is not used
N	LPC_PINCON->PORT0_0_to_15_FUNCTION   |= PIN15_as_SCK;		// SCK
X	((LPC_PINCON_TypeDef *) ((0x40000000UL) + 0x2C000) )->PINSEL0   |= (3UL << 30);		
N	LPC_PINCON->PORT0_16_to_31_FUNCTION  |= PIN17_as_MISO |		// MISO
X	((LPC_PINCON_TypeDef *) ((0x40000000UL) + 0x2C000) )->PINSEL1  |= (3UL << 2) |		
N                                           PIN18_as_MOSI;		// MOSI
X                                           (3UL << 4);		
N
N	/* Internal pull configure */
N	LPC_PINCON->PORT0_0_to_15_PULL_MODE  |= PIN15_NOPULL;
X	((LPC_PINCON_TypeDef *) ((0x40000000UL) + 0x2C000) )->PINMODE0  |= (2UL << 30);
N	LPC_PINCON->PORT0_16_to_31_PULL_MODE |= PIN17_NOPULL |
X	((LPC_PINCON_TypeDef *) ((0x40000000UL) + 0x2C000) )->PINMODE1 |= (2UL << 2) |
N                                            PIN18_NOPULL;
X                                            (2UL << 4);
N
N	/* Pin configure to SPI Chip Select lines */
N	LPC_PINCON->PORT0_16_to_31_FUNCTION  |= PIN24_as_GPIO |		// CS0
X	((LPC_PINCON_TypeDef *) ((0x40000000UL) + 0x2C000) )->PINSEL1  |= (0UL << 16) |		
N                                            PIN25_as_GPIO |		// CS1
X                                            (0UL << 18) |		
N                                            PIN26_as_GPIO |		// CS2
X                                            (0UL << 20) |		
N                                            PIN27_as_GPIO |		// CS3
X                                            (0UL << 22) |		
N                                            PIN28_as_GPIO;		// CS4
X                                            (0UL << 24);		
N
N	/* SC Internal pull configure */
N	LPC_PINCON->PORT0_16_to_31_PULL_MODE |= PIN24_NOPULL |
X	((LPC_PINCON_TypeDef *) ((0x40000000UL) + 0x2C000) )->PINMODE1 |= (2UL << 16) |
N                                            PIN25_NOPULL |
X                                            (2UL << 18) |
N                                            PIN26_NOPULL |
X                                            (2UL << 20) |
N                                            PIN27_NOPULL |
X                                            (2UL << 22) |
N                                            PIN28_NOPULL;
X                                            (2UL << 24);
N
N	/* CS direction */
N	LPC_GPIO0->PORT_DIRECTION            |= PIN24_OUTPUT |
X	((LPC_GPIO_TypeDef *) ((0x2009C000UL) + 0x00000) )->FIODIR            |= (1UL << 24) |
N                                            PIN25_OUTPUT |
X                                            (1UL << 25) |
N                                            PIN26_OUTPUT |
X                                            (1UL << 26) |
N                                            PIN27_OUTPUT |
X                                            (1UL << 27) |
N                                            PIN28_OUTPUT;
X                                            (1UL << 28);
N
N	/* Set CS (inactive state) */
N	LPC_GPIO0->PORT_SET                  |= FLASH_CS0 |
X	((LPC_GPIO_TypeDef *) ((0x2009C000UL) + 0x00000) )->FIOSET                  |= (1UL << 24) |
N                                            FLASH_CS1 |
X                                            (1UL << 25) |
N                                            FLASH_CS2 |
X                                            (1UL << 26) |
N                                            FLASH_CS3 |
X                                            (1UL << 27) |
N                                            FLASH_CS4;
X                                            (1UL << 28);
N
N	/* SPI Cotrol - 8 bits, CPOL=0, CPHA=0, Master mode, SPI Int enable */
N	LPC_SPI->SPCR = (1 << 7);
X	((LPC_SPI_TypeDef *) ((0x40000000UL) + 0x20000) )->SPCR = (1 << 7);
N}
N
