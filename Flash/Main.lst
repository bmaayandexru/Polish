N/** @brief interrupt type */
Ntypedef enum
N{
N	TIM_MR0_INT =0, /*!< interrupt for Match channel 0*/
N	TIM_MR1_INT =1, /*!< interrupt for Match channel 1*/
N	TIM_MR2_INT =2, /*!< interrupt for Match channel 2*/
N	TIM_MR3_INT =3, /*!< interrupt for Match channel 3*/
N	TIM_CR0_INT =4, /*!< interrupt for Capture channel 0*/
N	TIM_CR1_INT =5 /*!< interrupt for Capture channel 1*/
N}TIM_INT_TYPE;
N
N/** @brief Timer/counter operating mode */
Ntypedef enum
N{
N	TIM_TIMER_MODE = 0,				/*!< Timer mode */
N	TIM_COUNTER_RISING_MODE,		/*!< Counter rising mode */
N	TIM_COUNTER_FALLING_MODE,		/*!< Counter falling mode */
N	TIM_COUNTER_ANY_MODE			/*!< Counter on both edges */
N} TIM_MODE_OPT;
N
N/** @brief Timer/Counter prescale option */
Ntypedef enum
N{
N	TIM_PRESCALE_TICKVAL = 0,		/*!< Prescale in absolute value */
N	TIM_PRESCALE_USVAL				/*!< Prescale in microsecond value */
N} TIM_PRESCALE_OPT;
N
N/** @brief Counter input option */
Ntypedef enum
N{
N	TIM_COUNTER_INCAP0 = 0,			/*!< CAPn.0 input pin for TIMERn */
N	TIM_COUNTER_INCAP1,				/*!< CAPn.1 input pin for TIMERn */
N} TIM_COUNTER_INPUT_OPT;
N
N/** @brief Timer/Counter external match option */
Ntypedef enum
N{
N	TIM_EXTMATCH_NOTHING = 0,		/*!< Do nothing for external output pin if match */
N	TIM_EXTMATCH_LOW,				/*!< Force external output pin to low if match */
N	TIM_EXTMATCH_HIGH,				/*!< Force external output pin to high if match */
N	TIM_EXTMATCH_TOGGLE				/*!< Toggle external output pin if match */
N}TIM_EXTMATCH_OPT;
N
N/** @brief Timer/counter capture mode options */
Ntypedef enum {
N	TIM_CAPTURE_NONE = 0,	/*!< No Capture */
N	TIM_CAPTURE_RISING,		/*!< Rising capture mode */
N	TIM_CAPTURE_FALLING,	/*!< Falling capture mode */
N	TIM_CAPTURE_ANY			/*!< On both edges */
N} TIM_CAP_MODE_OPT;
N
N/** @brief Configuration structure in TIMER mode */
Ntypedef struct
N{
N
N	uint8_t PrescaleOption;		/**< Timer Prescale option, should be:
N									- TIM_PRESCALE_TICKVAL: Prescale in absolute value
N									- TIM_PRESCALE_USVAL: Prescale in microsecond value
N									*/
N	uint8_t Reserved[3];		/**< Reserved */
N	uint32_t PrescaleValue;		/**< Prescale value */
N} TIM_TIMERCFG_Type;
N
N/** @brief Configuration structure in COUNTER mode */
Ntypedef struct {
N
N	uint8_t CounterOption;		/**< Counter Option, should be:
N								- TIM_COUNTER_INCAP0: CAPn.0 input pin for TIMERn
N								- TIM_COUNTER_INCAP1: CAPn.1 input pin for TIMERn
N								*/
N	uint8_t CountInputSelect;
N	uint8_t Reserved[2];
N} TIM_COUNTERCFG_Type;
N
N/** @brief Match channel configuration structure */
Ntypedef struct {
N	uint8_t MatchChannel;	/**< Match channel, should be in range
N							from 0..3 */
N	uint8_t IntOnMatch;		/**< Interrupt On match, should be:
N							- ENABLE: Enable this function.
N							- DISABLE: Disable this function.
N							*/
N	uint8_t StopOnMatch;	/**< Stop On match, should be:
N							- ENABLE: Enable this function.
N							- DISABLE: Disable this function.
N							*/
N	uint8_t ResetOnMatch;	/**< Reset On match, should be:
N							- ENABLE: Enable this function.
N							- DISABLE: Disable this function.
N							*/
N
N	uint8_t ExtMatchOutputType;	/**< External Match Output type, should be:
N							 -	 TIM_EXTMATCH_NOTHING:	Do nothing for external output pin if match
N							 -   TIM_EXTMATCH_LOW:	Force external output pin to low if match
N							 - 	 TIM_EXTMATCH_HIGH: Force external output pin to high if match
N							 -   TIM_EXTMATCH_TOGGLE: Toggle external output pin if match.
N							*/
N	uint8_t Reserved[3];	/** Reserved */
N	uint32_t MatchValue;	/** Match value */
N} TIM_MATCHCFG_Type;
N
N/** @brief Capture Input configuration structure */
Ntypedef struct {
N	uint8_t CaptureChannel;	/**< Capture channel, should be in range
N							from 0..1 */
N	uint8_t RisingEdge;		/**< caption rising edge, should be:
N							- ENABLE: Enable rising edge.
N							- DISABLE: Disable this function.
N							*/
N	uint8_t FallingEdge;		/**< caption falling edge, should be:
N							- ENABLE: Enable falling edge.
N							- DISABLE: Disable this function.
N								*/
N	uint8_t IntOnCaption;	/**< Interrupt On caption, should be:
N							- ENABLE: Enable interrupt function.
N							- DISABLE: Disable this function.
N							*/
N
N} TIM_CAPTURECFG_Type;
