N#define REGION_Enabled  (0x01)
N#define REGION_32K      (14 << 1)      // 2**15 == 32k
N		#define REGION_64K      (15 << 1)      // 2**15 == 32k
N		#define REGION_16K      (13 << 1)      // 2**15 == 32k
N#define REGION_512K      (18 << 1)      // 2**15 == 32k
N#define REGION_1M      (19 << 1)      // 2**15 == 32k
N#define REGION_2M      (20 << 1)      // 2**15 == 32k
N
N#define REGION_Valid 0x10
N#define NORMAL          (8 << 16)      // TEX:0b001 S:0b0 C:0b0 B:0b0
N#define DEVICE_NON_SHAREABLE          (16 << 16)      // TEX:0b001 S:0b0 C:0b0 B:0b0
N#define STRONGLY_ORDERED_SHAREABLE          (0 << 16)      // TEX:0b001 S:0b0 C:0b0 B:0b0
N
N#define FULL_ACCESS     (0x03 << 24)   // Privileged Read Write, Unprivileged Read Write
N#define RO     (0x06 << 24)   // Privileged Read Write, Unprivileged Read Write
N#define NOT_EXEC        (0x01 << 28)   // All Instruction fetches abort		
N
Nvoid lpc1768_mpu_config(void)
N{
N   /* Disable MPU */
N   MPU->CTRL = 0;
X   ((MPU_Type *) ((0xE000E000UL) + 0x0D90UL) )->CTRL = 0;
N   /* Configure region 0 to cover 512KB Flash (Normal, Non-Shared, Executable, Read-only) */
N   MPU->RBAR = 0x00000000 | REGION_Valid | 0;
X   ((MPU_Type *) ((0xE000E000UL) + 0x0D90UL) )->RBAR = 0x00000000 | 0x10 | 0;
N   MPU->RASR = REGION_Enabled |  REGION_512K | RO;
X   ((MPU_Type *) ((0xE000E000UL) + 0x0D90UL) )->RASR = (0x01) |  (18 << 1) | (0x06 << 24);
N   /* Configure region 1 to cover CPU 32KB SRAM (Normal, Non-Shared, Executable, Full Access) */
N   MPU->RBAR = 0x10000000 | REGION_Valid | 1;
X   ((MPU_Type *) ((0xE000E000UL) + 0x0D90UL) )->RBAR = 0x10000000 | 0x10 | 1;
N   MPU->RASR = REGION_Enabled |  REGION_32K | FULL_ACCESS;
X   ((MPU_Type *) ((0xE000E000UL) + 0x0D90UL) )->RASR = (0x01) |  (14 << 1) | (0x03 << 24);
N   /* Configure region 2 to cover AHB 32KB SRAM (Normal, Non-Shared, Executable, Full Access) */
N   MPU->RBAR = 0x2007C000 | REGION_Valid | 2;
X   ((MPU_Type *) ((0xE000E000UL) + 0x0D90UL) )->RBAR = 0x2007C000 | 0x10 | 2;
N   MPU->RASR = REGION_Enabled | REGION_32K | FULL_ACCESS;
X   ((MPU_Type *) ((0xE000E000UL) + 0x0D90UL) )->RASR = (0x01) | (14 << 1) | (0x03 << 24);
N   /* Configure region 3 to cover 16KB GPIO (Device, Non-Shared, Full Access Device, Full Access) */
N   MPU->RBAR = 0x2009C000 | REGION_Valid | 3;
X   ((MPU_Type *) ((0xE000E000UL) + 0x0D90UL) )->RBAR = 0x2009C000 | 0x10 | 3;
N   MPU->RASR = REGION_Enabled | REGION_16K | FULL_ACCESS;
X   ((MPU_Type *) ((0xE000E000UL) + 0x0D90UL) )->RASR = (0x01) | (13 << 1) | (0x03 << 24);
N   /* Configure region 4 to cover 512KB APB Peripherials (Device, Non-Shared, Full Access Device, Full Access) */
N   MPU->RBAR = 0x40000000 | REGION_Valid | 4;
X   ((MPU_Type *) ((0xE000E000UL) + 0x0D90UL) )->RBAR = 0x40000000 | 0x10 | 4;
N   MPU->RASR = REGION_Enabled |  REGION_1M | FULL_ACCESS;
X   ((MPU_Type *) ((0xE000E000UL) + 0x0D90UL) )->RASR = (0x01) |  (19 << 1) | (0x03 << 24);
N   /* Configure region 5 to cover 2MB AHB Peripherials (Device, Non-Shared, Full Access Device, Full Access) */
N   MPU->RBAR = 0x50000000 | REGION_Valid | 5;
X   ((MPU_Type *) ((0xE000E000UL) + 0x0D90UL) )->RBAR = 0x50000000 | 0x10 | 5;
N   MPU->RASR = REGION_Enabled |  REGION_2M | FULL_ACCESS;
X   ((MPU_Type *) ((0xE000E000UL) + 0x0D90UL) )->RASR = (0x01) |  (20 << 1) | (0x03 << 24);
N   /* Configure region 6 to cover the 1MB PPB (Privileged, XN, Read-Write) */
N   MPU->RBAR = 0xE0000000 | REGION_Valid | 6;
X   ((MPU_Type *) ((0xE000E000UL) + 0x0D90UL) )->RBAR = 0xE0000000 | 0x10 | 6;
N   MPU->RASR = REGION_Enabled | REGION_1M | FULL_ACCESS;
X   ((MPU_Type *) ((0xE000E000UL) + 0x0D90UL) )->RASR = (0x01) | (19 << 1) | (0x03 << 24);
N   /* Enable MPU */
N
N   MPU->CTRL = 1;
X   ((MPU_Type *) ((0xE000E000UL) + 0x0D90UL) )->CTRL = 1;
N   __ISB();
X   do { __schedule_barrier(); __isb(0xF); __schedule_barrier(); } while (0U);
N  __DSB();
X  do { __schedule_barrier(); __dsb(0xF); __schedule_barrier(); } while (0U);
N}		
