N// Вычислить контрольную сумму CRC8
N// ВНИМАНИЕ! Идем по буферу назад!
Nu8 CRC8(u8 *ptr, u8 crc, u16 len) {
N	while (len--) {
N		crc = Table_CRC8[crc ^ *ptr--];
N	}
N	return crc;
N}
N
Nfloat sqr(float x) {
N	return x*x;
N}
N
Nint Round(float f){
N	if ((f-(int)f) > 0.5) {
N		return (int)f+1;
N	} else {
N		return (int)f;
N	}
N}
N
Ns32 abss32(s32 a) {
N	if (a < 0) return -a;
N	else return a;
N}
