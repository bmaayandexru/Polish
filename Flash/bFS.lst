Ns8 bFS_Read(tFile fcs_num, u8 *buffer, u32 size, u32 * rbc) {
N	u32 i;
N	s8 res;
N	if (size == 0) return FS_RESULT_OK;
X	if (size == 0) return 0;
N	*rbc = 0;
N	for (i=0;i < size;i++){
N		res = FS_ReadByte(fcs_num,buffer+i);
N		if (res < 0) {
N			return res;
N		} else {
N			(*rbc)++;
N		}
N	}	
N	return res;
N}
N
N
Ns8 bFS_ReadLn(tFile fcs_num, u8 *str, u32 max_size) {
N	u32 i;
N	u8 s;
N	u8 b;
N	s8 res;
N
N	s = 0; 
N	i = 0;
N	if (FS_Eof(fcs_num)==FS_EOF) return FS_EOF;
X	if (FS_Eof(fcs_num)== -21) return -21;
N	do {
N		res = FS_ReadByte(fcs_num,&b);
N		if (res < 0) return res;
N		else {
N			switch (s) {
N				case 0:
N					// читаем все символы до 13
N					*(str+i) = b;
N					if (b == 13) s=1;
N				break;
N	
N				case 1:
N					if (b == 10) {
N						// следующий симпол за 13 должен быть 10
N						*(str+i) = b;
N						// выходим
N						return FS_RESULT_OK;
X						return 0;
N					} else {
N						// следующий за 13 символ не 10
N						FS_RSeek(fcs_num,-1);
N						// сдвигаемся на предыдущий символ и выходим
N						return FS_RESULT_OK;
X						return 0;
N					}
N//				break;
N			}
N		}
N		i++;
N	} while	(i<max_size);
N	// если переехали max_size тоже выходим
N	return FS_RESULT_OK;
X	return 0;
N}
N
Ns8 FS_ReadPrevByte(tFile fcs_num,u8 * b){
N	s32 pos;
N	s8 res;
N	res = FS_Pos(fcs_num,&pos);
N	if (res) return res;
N	// получили позицию в pos
N	if (pos) {
N		res = FS_RSeek(fcs_num,-1);
N		if (res) return res;
N		res = FS_ReadByte(fcs_num,b);
N		if (res) return res;
N		res = FS_RSeek(fcs_num,-1);
N		return res;
N	} else {
N		*b = 0;
N		return FS_BOF;
X		return -22;
N	}
N}
N
Ns8 bFS_BReadLn(tFile fcs_num, u8 *str, u32 max_size) {
N// прочитать строку в обратном направлении
N	u8 s;
N	u8 b;
N	s8 res;
N	s32 fixpos;
N	s32 pos;
N
N	s = 0; 
N
N	// если стоим в начале то ничего вычитать не можем
N	res = FS_Pos(fcs_num,&pos);
N	if (res) return res;
N	if (pos==0) return FS_BOF; // выходим
X	if (pos==0) return -22; 
N
N	// идем по файлу в обратном направлении
N	// до начала или до 2-х вхождений символа 13
N	while ((FS_ReadPrevByte(fcs_num,&b)==0)&&(s < 2)) {
N		if (b == 13) s++;
N	}
N	res = FS_Pos(fcs_num,&pos);
N	if (res) return res;
N	if (pos) {
N		// если не в начале файла
N		// вычитываем три символа XX 13 10
N		FS_ReadByte(fcs_num,&b);
N		FS_ReadByte(fcs_num,&b);
N		FS_ReadByte(fcs_num,&b);
N	}	
N	// здесь гарантированно стоим на первом символе пред строки
N	// фиксирум позицию чтобы вернуться в нее
N	res = FS_Pos(fcs_num,&fixpos);
N	if (res) return res;
N	// читаем строку
N	res = bFS_ReadLn(fcs_num,str,max_size);
N	if (res) return res;
N	// возвращаемся в позицию
N	res = FS_Seek(fcs_num,fixpos);
N	return res;
N}
N
Ns8 bFS_Write(tFile fcs_num, const u8 *buffer, u32 size, u32 * wbc) {
N	u32 i;
N	s8 res;
N	*wbc = 0;
N	if (size == 0) return FS_RESULT_OK;
X	if (size == 0) return 0;
N	for (i=0;i<size;i++) {
N		res = FS_WriteByte(fcs_num,*(buffer+i));
N//		res = 0;
N		if (res < 0) {
N			return res;
N		} else {
N			(*wbc)++;
N		}
N	}
N	return res;
N}
