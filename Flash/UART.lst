N			if (!__BUF_IS_FULL(rb0.rx_head,rb0.rx_tail)){
X			if (!((rb0 . rx_tail&(512-1))==((rb0 . rx_head+1)&(512-1)))){
N				rb0.rx[rb0.rx_head] = tmpc;
N				__BUF_INCR(rb0.rx_head);
X				(rb0 . rx_head=(rb0 . rx_head+1)&(512-1));
N				rb0.rxdata++;
N			}
N		}
N		// no more data
N		else {
N			break;
N		}
N	}
