Nvoid UART0_IntReceive(void){
N	uint8_t tmpc;
N	uint32_t rLen;
N
N	while(1){
N		// Call UART read function in UART driver
N		rLen = UART_Receive((LPC_UART_TypeDef *)LPC_UART0, &tmpc, 1, NONE_BLOCKING);
